---
apiVersion: cert-manager.io/v1
kind: Certificate
metadata:
  name: catalog-webhook-cert
  namespace: catalog
spec:
  secretName: catalog-webhook-cert 
  isCA: false
  usages:
    - digital signature
    - key encipherment
    - server auth
    - client auth
  dnsNames:
    - "catalog-catalog-webhook.catalog.svc"
  issuerRef:
    kind: ClusterIssuer
    group: cert-manager.io
    name: tmaxcloud-issuer
